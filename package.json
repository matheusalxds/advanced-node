{
  "name": "advanced-clean-node",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Matheus Alexandre <mathe.system@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node -r dotenv/config dist/main",
    "start:dev": "yarn build && yarn start",
    "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config --inspect=0.0.0.0:9229 src/main",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm t -- --watch",
    "test:staged": "npm t -- --findRelatedTests",
    "test:coverage": "npm t -- --coverage",
    "test:fb-api": "npm  t -- -c ./jest.integration.config.js tests/infra/external/facebook-api.test.ts",
    "test:s3": "npm  t -- -c ./jest.integration.config.js tests/infra/external/aws-s3-file-storage.test.ts",
    "jest:clearCache": "jest --clearCache",
    "check": "npm-check -s -u",
    "prepare": "husky install"
  },
  "dependencies": {
    "aws-sdk": "^2.1076.0",
    "axios": "^0.26.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "multer": "^1.4.4",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "typeorm": "^0.2.43",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@jest-mock/express": "^1.4.5",
    "@types/aws-sdk": "^2.7.0",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/module-alias": "^2.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.18",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "eslint": "^7.12.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-mock": "^27.5.1",
    "jest-mock-extended": "^2.0.4",
    "lint-staged": "^12.3.4",
    "mockdate": "^3.0.5",
    "npm-check": "^5.9.2",
    "pg-mem": "^2.3.3",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": "16.x"
  }
}
